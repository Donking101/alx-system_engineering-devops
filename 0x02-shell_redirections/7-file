#!/bin/bash

# Define the filename
filename=```'\*\\'"Best School"'\\*\$\?\\*\\*\\*\\*\\*:)'```
# \*\\'"Best School"\'\\*$\?\*\*\*\*\*:) 
# Define the content
content="Best School\n"

# Create the file with the specified name and content
echo -e "$content" > "$filename"

echo "File created: $filename"
